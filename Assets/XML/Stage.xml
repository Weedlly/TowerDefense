<?xml version="1.0" encoding="utf-8" ?>
<StageList>
	<Stage id="StageID">

		<UnitList>
			<Unit id="UnitID">
				<Name>Tên của lính</Name>
				<Attribute>Thuộc tính chi tiết (Dame, tầm đánh,..)</Attribute>
			</Unit>
            <!-- Các Unit tiếp theo -->
		</UnitList>

		<Gates>
			<Gate id="GateID">
				<LocationList>Toạ độ đường đi từ cổng đến đích</LocationList>
			</Gate>
            <!-- Các Gate tiếp theo -->
		</Gates>

		<TowerPlaceList>
			<TowerPlace>Vị trí đặt tháp</TowerPlace>
            <!-- Các Vị trí tiếp theo -->
		</TowerPlaceList>

		<WaveList>
			<Wave>
				<Subwaves>
					<Subwave>
						<AppearTime>Thời gian bắt đầu xuất hiện</AppearTime>
						<GateId>Cổng xuất hiện</GateId>
						<UnitID>Loại lính xuất hiện</UnitID>
					</Subwave>
                    <!-- Các Subwave tiếp theo -->
				</Subwaves>
			</Wave>
            <!-- Các Wave tiếp theo -->
		</WaveList>

	</Stage>
    <!-- Các Stage tiếp theo -->
</StageList>